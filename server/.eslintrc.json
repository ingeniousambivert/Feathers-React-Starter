{
	"extends": "eslint:recommended",
	"env": {
		"es6": true,
		"node": true,
		"jest": true
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		],
		"arrow-parens": [
			"error",
			"always"
		],
		"generator-star-spacing": [
			"error",
			"after"
		]
	}
}
